<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap 
   PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" 
   "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
   
 <sqlMap>
	<select id="getCustomerById" resultClass="com.sodimac.automation.model.Customer">
		select 
			idcustomer,
		    name
		from 
			customer
		where idCustomer = #id#;
	</select>
	
